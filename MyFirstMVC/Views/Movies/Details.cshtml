@model MyFirstMVC.Models.Movie
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Details</h2>
<ul>
    <li>Genre: @Model.Genres.GenreType</li>
    <li>Release Date: @Model.ReleaseDate</li>
    <li>Added Date: @Model.DateAdded</li>
    <li>Number Of Copies: @Model.NumberOfCopies</li>
</ul>

